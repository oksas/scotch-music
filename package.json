{
  "name": "scotch-music",
  "version": "1.0.0",
  "description": "SoundCloud player built alongside a Scotch.io tutorial",
  "main": "index.js",
  "scripts": {
    "start": "electron main.js",
    "watch": "watchify app/app.js -t babelify -o public/js/bundle.js --debug --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oksas/scotch-music.git"
  },
  "author": "John Oksasoglu",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/oksas/scotch-music/issues"
  },
  "homepage": "https://github.com/oksas/scotch-music#readme",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "axios": "^0.11.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "classnames": "^2.2.4",
    "electron-prebuilt": "^0.37.8",
    "electron-reload": "^0.3.0",
    "react": "^0.14.8",
    "react-autocomplete": "^1.0.0-rc2",
    "react-dom": "^0.14.8",
    "react-sound": "^0.4.0",
    "soundmanager2": "^2.97.20150601-a"
  }
}
